{
  "name": "tailwind-nextjs-starter-blog",
  "version": "1.0.0-canary.0",
  "private": true,
  "scripts": {
    "start": "next-remote-watch ./data",
    "compose": "node scripts/compose",
    "dev": "next dev -p 8888",
    "build": "next build && node ./scripts/generate-sitemap",
    "serve": "next start",
    "analyze": "cross-env ANALYZE=true next build",
    "lint": "next lint --fix --dir pages --dir components --dir lib --dir layouts --dir scripts",
    "prepare": "husky install"
  },
  "dependencies": {
    "@tailwindcss/forms": "^0.5.9",
    "@tailwindcss/typography": "^0.5.15",
    "autoprefixer": "^10.4.20",
    "gray-matter": "^4.0.3",
    "image-size": "1.1.1",
    "mdx-bundler": "^10.0.3",
    "next": "14.2.14",
    "next-themes": "^0.3.0",
    "postcss": "^8.4.47",
    "preact": "^10.24.2",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "reading-time": "1.5.0",
    "rehype-autolink-headings": "^7.1.0",
    "rehype-katex": "^7.0.1",
    "rehype-prism-plus": "2.0.0",
    "rehype-slug": "^6.0.0",
    "remark-autolink-headings": "8.0.0",
    "remark-footnotes": "^5.0.0",
    "remark-gfm": "^4.0.0",
    "remark-math": "^6.0.0",
    "remark-slug": "8.0.0",
    "tailwindcss": "^3.4.13"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "14.2.14",
    "@svgr/webpack": "^8.1.0",
    "cross-env": "^7.0.3",
    "dedent": "^1.5.3",
    "eslint": "^8.57.1",
    "eslint-config-next": "14.2.14",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-jsx-a11y": "^6.10.0",
    "eslint-plugin-prettier": "^5.2.1",
    "file-loader": "^6.2.0",
    "globby": "14.0.2",
    "husky": "^9.1.6",
    "inquirer": "^12.0.0",
    "lint-staged": "^15.2.10",
    "next-remote-watch": "^2.0.0",
    "prettier": "3.3.3",
    "rehype": "13.0.2",
    "remark-frontmatter": "5.0.0",
    "remark-parse": "11.0.0",
    "remark-stringify": "11.0.0",
    "unified": "11.0.5",
    "unist-util-visit": "5.0.0"
  },
  "lint-staged": {
    "*.+(js|jsx|ts|tsx)": [
      "eslint --fix"
    ],
    "*.+(js|jsx|ts|tsx|json|css|md|mdx)": [
      "prettier --write"
    ]
  }
}
